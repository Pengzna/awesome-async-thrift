namespace java com.timecho.aweseme.thrift

struct TEndPoint {
  1: required string ip
  2: required i32 port
}

struct TDNodeConfiguration {
  1: required TEndPoint targetCNode
  2: required string requestType
  3: required i32 clientNum
  4: required i32 requestNum
}
